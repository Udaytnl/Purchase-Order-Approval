<core:FragmentDefinition xmlns:core="sap.ui.core" controllerName="com.sap.build.standard.poMd.controller.Dialog1" xmlns="sap.m">
    <Dialog horizontalScrolling="true" showHeader="true" state="None" stretch="false" title="Filter Values" type="Standard" verticalScrolling="true">
       
       <content>
        		<Label text="Group Codes:" width="auto" class="sapUiLargeMargin" ></Label>
        		<ViewSettingsDialog
		confirm="handleConfirm">
       	<filterItems>
			<ViewSettingsFilterItem text="Products" key="1">
				<items>
					<ViewSettingsItem text="Mouse DX20" key="1a" />
					<ViewSettingsItem text="Optical mouse OX3" key="1b" />
					<ViewSettingsItem text="Monitor" key="1c" />
					<ViewSettingsItem text="Keyboard (ultra flat)" key="1d" />
					<ViewSettingsItem text="Wireless keyboard" key="1e" />
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
        </content>
       
       
        
        <beginButton/>
        <buttons>
            <Button text="OK" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press = "ok"/>
            <Button text="Cancel" type="Default" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="cancel"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>
